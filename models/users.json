{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "id":{
              "default":0
        },
        "username": {
          "type": "string"
        },
        "contact number": {
          "type": "string",
          "pattern": "^\\d{10}$"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "password": {
          "type": "string"
        }
      },
      "required": ["name", "contact number", "email", "password"]
    }
  },
  "required": ["user"]
}